{
  "name": "nikin.design",
  "version": "4.0.0",
  "description": "Portfolio website for Nikin Nagewadia",
  "main": "eleventy.config.js",
  "scripts": {
    "watch:sass": "sass --watch src/scss:_site/css --style=compressed",
    "build:sass": "sass src/scss:_site/css --style=compressed",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "concurrently \"npm run build:sass\" \"npm run watch:*\"",
    "production": "concurrently \"npm run build:*\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-fetch": "^5.0.2",
    "@11ty/eleventy-img": "^6.0.1",
    "@11ty/eleventy-plugin-rss": "^2.0.3",
    "html-minifier-terser": "^7.2.0",
    "luxon": "^3.5.0",
    "sass": "^1.83.4",
    "terser": "^5.38.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
